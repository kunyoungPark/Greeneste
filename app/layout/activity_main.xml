<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LogIn">

    <TextView
        android:id="@+id/main_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:gravity="center_horizontal"
        android:padding="2dp"
        android:text="Greeneste"
        android:textSize="50dp"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="239dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_text_btn"
        android:layout_width="350dp"
        android:layout_height="57dp"
        android:layout_margin="15dp"
        android:backgroundTint="@color/black"
        android:gravity="center"
        android:text="Sign in"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="450dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_text_btn2"
        android:layout_width="350dp"
        android:layout_height="57dp"
        android:backgroundTint="@color/black"
        android:gravity="center"
        android:text="facebook"
        android:layout_margin="15dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="534dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_text_btn3"
        android:layout_width="350dp"
        android:layout_height="57dp"
        android:backgroundTint="@color/black"
        android:gravity="center"
        android:text="Email/Gmail"
        android:layout_margin="15dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="614dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>